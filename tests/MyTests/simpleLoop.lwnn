class Main {
  def Main(a:A) = {
    a := new A(1,2);
    a.set(4, 5);
    while( 0 < a.a) {
      a.set(a.a - 1, a.b - 1);
    };
    print a.a;
    print a.b;
  }
}

class A {
  fields a : int, b: int;
  def A(x:int, y:int) = {
    self.a := x;
    self.b := y;
  }
  def set(x: int, y: int) = {
    self.a := x;
    self.b := y;
  }
}