## simple string comparison/manipulation ##
class Main {
    def Main(a: string, b: string, sc: StringCompare, t1: bool) = {
        a := {"abc", "xyz"};
        b := {"abc", "xyz"};
        sc := new StringCompare();

        print a;
        print b;
        print "Concatenate: " + a + b;

        t1 := sc.equality(a, b);
        t1 := sc.lessThan(a, b);
        t1 := sc.lessThanStrict(a, b);

    }


}

class StringCompare {
 def equality(a: string, b: string, c: bool): bool = {
          if(a = b){
             print "The two strings are equal.";
             c := true;

          }else {
             print "The two strings are not equal.";
             c := false;
          };

          return c;
     }

 def lessThan(a: string, b: string, c: bool): bool = {

        if (a <= b){
             print a + " is <= " + b;
             c := true;
         }else {
             print a + " is not <= " + b;
             c := false;

         };

         return c;
 }


 def lessThanStrict(a: string, b: string, c: bool): bool = {
         if (a < b){
            print a + " is < " + b;
            c := true;
         }else {
            print a + " is not < " + b;
            c := false;

         };

         return c;
 }
}
