class Main {
  def Main(x:Shape) = {
    x := new Square();
  }
}

class Shape {
  fields color: string;

  def setColor(color: string) = { self.color := color; }
  def getColor(): string = { return self.color; }
  def draw() = { print self.color; }
}

class Square extends Shape {
  def Square() = {
    self.color := "Hello World!";
    self.draw();
  }
}
